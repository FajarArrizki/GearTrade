/**
 * For more details on how to configure Wrangler, refer to:
 * https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "geartrade",
  "main": "src/index.ts",
  "compatibility_date": "2025-06-01",
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": [
        "TradingAgent"
      ]
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "AGENT",
        "class_name": "TradingAgent"
      }
    ]
  },
  "observability": {
    "enabled": true
  },
  "vars": {
    "TEST_MODE": "true",
    "AI_PROVIDER": "zai",
    "MODEL_ID": "glm-4.5",
    "ARBITRUM_RPC_URL": "https://sepolia-rollup.arbitrum.io/rpc",
    "SESSION_REGISTRY_ADDRESS": "0x0000000000000000000000000000000000000000",
    "TRADING_VAULT_ADDRESS": "0x0000000000000000000000000000000000000000",
    "HYPERLIQUID_BASE_URL": "wss://api.hyperliquid.xyz/ws",
    "HYPERLIQUID_API_URL": "https://api.hyperliquid.xyz",
    "HYPERLIQUID_RPC_URL": "https://api.hyperliquid.xyz",
    "HYPERLIQUID_WALLET_ADDRESS": "0xef63654b60b30a3f4a894ce974d96506c98d0871",
    "HYPERLIQUID_ACCOUNT_ADDRESS": "0x464BF4046f2c71CbB67483E2Ff23640D21199A1C",
    "MCP_SCRAPER_USE_SAVE_SHORTCUT": "true",
    "MCP_SCRAPER_HEADLESS": "true",
    "MCP_SCRAPER_WINDOW_WIDTH": "1400",
    "MCP_SCRAPER_WINDOW_HEIGHT": "1400"
  },
  "services": [
    {
      "binding": "MCP_SERVICE",
      "service": "mcp"
    }
  ],
  "compatibility_flags": [
    "nodejs_compat"
  ]
}
